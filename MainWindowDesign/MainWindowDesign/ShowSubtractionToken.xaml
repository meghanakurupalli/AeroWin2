<Window x:Class="MainWIndowDesign.ShowSubtractionToken"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MainWIndowDesign"
        mc:Ignorable="d"
        Title="ShowSubtractionToken" Width="350">
    <Grid>
        <DataGrid>
            <DataGrid.Columns>
                <DataGridTextColumn Header="Index" Binding="{Binding Indices}"></DataGridTextColumn>
                <DataGridTextColumn Header="Air flow" Binding="{Binding Airflows}"></DataGridTextColumn>
                <DataGridTextColumn Header="Pressure" Binding="{Binding Pressures}"></DataGridTextColumn>
                <DataGridTextColumn Header="Resistance" Binding="{Binding Resistances}"></DataGridTextColumn>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
